{
  "name": "datachan",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "release": "cd android && gradlew assembleRelease",
    "clean": "cd android && gradlew clean",
    "preandroid": "npm run ts",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "bundle:android": "node node_modules/react-native/local-cli/cli.js bundle --platform android --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/ --dev false --sourcemap-sources-root=build",
    "ts": "tsc -p .",
    "tsw": "tsc -p . --w",
    "test": "jest --no-cache",
    "unit-test": "tsc -p __unit_tests__ && mocha __unit_tests__/",
    "postinstall": "rn-nodeify --install stream,crypto,vm,buffer,events,assert --hack",
    "generator": "./node_modules/.bin/generator -d d:/www/ERA/datachan/resources/images/ -o d:/www/ERA/datachan/src/common/ImageRecources.g.ts"
  },
